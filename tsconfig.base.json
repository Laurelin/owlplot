{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["ESNext"],
      "module": "ES2022",
      "moduleResolution": "Bundler",
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
  }